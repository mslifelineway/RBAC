{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/product/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/product/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/product",
  "projects": {
    "administrator": {
      "type": "application",
      "root": "apps/administrator",
      "entryFile": "main",
      "sourceRoot": "apps/administrator/src",
      "compilerOptions": {
        "tsConfigPath": "apps/administrator/tsconfig.app.json"
      }
    },
    "employee": {
      "type": "application",
      "root": "apps/employee",
      "entryFile": "main",
      "sourceRoot": "apps/employee/src",
      "compilerOptions": {
        "tsConfigPath": "apps/employee/tsconfig.app.json"
      }
    },
    "product": {
      "type": "application",
      "root": "apps/product",
      "entryFile": "main",
      "sourceRoot": "apps/product/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "permission": {
      "type": "application",
      "root": "apps/permission",
      "entryFile": "main",
      "sourceRoot": "apps/permission/src",
      "compilerOptions": {
        "tsConfigPath": "apps/permission/tsconfig.app.json"
      }
    }
  }
}